#include <bits/stdc++.h>
#define fi first
#define se second
 
using namespace std;
using ll = long long;
const int Mod = 1e9+9;

ll dp[1005][1005][11];
 
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    //code here
    int n, m, k;
    cin >> n >> m >> k;
    int a[n+1], b[m+1];
    for (int i = 1; i <= n; i++) cin >> a[i];
    for (int j = 1; j <= m; j++) cin >> b[j];
    sort(a+1,a+1+n);
    sort(b+1,b+1+m);
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= m; j++) {
            dp[i][j][0] = 1;
        }
    }
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            for (int t = 1; t <= k; t++) {
                dp[i][j][t] = (dp[i-1][j][t] + dp[i][j-1][t] - dp[i-1][j-1][t] + Mod) % Mod;
                if (a[i] > b[j]) dp[i][j][t] = (dp[i][j][t] + dp[i-1][j-1][t-1]) % Mod;
            }
        }
    }
    cout << dp[n][m][k] << '\n';
}
