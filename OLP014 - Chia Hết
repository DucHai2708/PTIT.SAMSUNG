#include <bits/stdc++.h>
#define fi first
#define se second
 
using namespace std;
using ll = long long;
const int Mod = 1e9+7;

void pt(ll n, map<ll,int> &mp) {
    for (ll i = 2; i*i <= n; i++) {
        if (n % i == 0) {
            while (n % i == 0) {
                mp[i]++;
                n /= i;
            }
        }
    }
    if (n > 1) mp[n]++;
}

bool check(map<ll,int> A, map<ll,int> B) {
    for (auto x : B) {
        if (A[x.fi] == 0 || A[x.fi] < x.se) return false;
    } 
    return true;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    //code here
    int n, m;
    cin >> n >> m;
    vector<ll> a(n);
    map<ll,int> A, B;
    for (int i = 0; i < n; i++) {
        cin >> a[i];
        pt(a[i],A);
    }
    vector<ll> b(n);
    vector<ll> ans;
    for (int i = 1; i <= m; i++) {
        B.clear();
        for (int j = 0; j < n; j++) {
            cin >> b[j];
            pt(b[j],B);
        }
        if (check(A,B)) ans.push_back(i);
    }
    cout << ans.size() << '\n';
    for (ll x : ans) cout << x << ' ';
    cout << '\n';
}
