#include <bits/stdc++.h>
#define fi first
#define se second
 
using namespace std;
using ll = long long;
const int Mod = 1e9+7;

map<ll,int> pt(ll n) {
    map<ll,int> mp;
    for (ll i = 2; i*i <= n; i++) {
        if (n % i == 0) {
            while (n % i == 0) {
                mp[i]++;
                n /= i;
            }
        }
    }
    if (n > 1) mp[n]++;
    return mp;
}

ll get(map<ll,int> mp) {
    ll res = 1;
    for (auto x : mp) {
        res *= (1ll*x.se+1);
    }
    --res;
    return res;
}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    //code here
    int tc; cin >> tc;
    while (tc--) {
        ll m; cin >> m;
        map<ll,int> mp;
        for (int i = 0; i <= 2; i++) {
            map<ll,int> mp1 = pt(m+i);
            for (auto x : mp1) {
                mp[x.fi] += x.se;
            }
        }
        ll d = get(mp);
        for (auto x : mp) {
            mp[x.fi] *= 2;
        }
        ll e = get(mp);
        cout << (e - (d*2))/2 << '\n';
    }
}
