#include <bits/stdc++.h>
#define fi first
#define se second
 
using namespace std;
using ll = long long;
const int Mod = 1e9+7;

void pt(ll n) {
    vector<pair<ll,int>> v;
    for (ll i = 2; i*i <= n; i++) {
        if (n % i == 0) {
            int cnt = 0;
            while (n % i == 0) {
                cnt++;
                n /= i;
            }
            v.push_back({i,cnt});
        }
    }
    if (n > 1) v.push_back({n,1});
    for (auto x : v) cout << x.fi << ' ' << x.se << '\n';
    cout << '\n';
}
 
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL); cout.tie(NULL);
    //code here
    int tc; cin >> tc;
    while (tc--) {
        ll n; cin >> n;
        pt(n);
    }
}
